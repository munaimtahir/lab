    "playwright": "playwright test",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.55.0",
    "react-router-dom": "^7.1.0",
    "zod": "^3.23.8"
